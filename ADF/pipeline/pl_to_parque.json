{
	"name": "pl_to_parque",
	"properties": {
		"activities": [
			{
				"name": "Save to parque",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"blockSizeInMB": 4
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "S_SUPPKEY",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "S_SUPPKEY",
									"type": "Int32",
									"physicalType": "INT_32"
								}
							},
							{
								"source": {
									"name": "S_NAME",
									"type": "String",
									"physicalType": "char"
								},
								"sink": {
									"name": "S_NAME",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "S_ADDRESS",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "S_ADDRESS",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "S_NATIONKEY",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "S_NATIONKEY",
									"type": "Int32",
									"physicalType": "INT_32"
								}
							},
							{
								"source": {
									"name": "S_PHONE",
									"type": "String",
									"physicalType": "char"
								},
								"sink": {
									"name": "S_PHONE",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "S_ACCTBAL",
									"type": "Decimal",
									"physicalType": "decimal",
									"scale": 2,
									"precision": 15
								},
								"sink": {
									"name": "S_ACCTBAL",
									"type": "Decimal",
									"physicalType": "DECIMAL"
								}
							},
							{
								"source": {
									"name": "S_COMMENT",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "S_COMMENT",
									"type": "String",
									"physicalType": "UTF8"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_db_table",
						"type": "DatasetReference",
						"parameters": {
							"TableName": "supplier",
							"SchemaName": "g1"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_blob_parquet",
						"type": "DatasetReference",
						"parameters": {
							"folder": {
								"value": "@pipeline().parameters.folder",
								"type": "Expression"
							},
							"file": {
								"value": "supplier.parquet",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"folder": {
				"type": "string",
				"defaultValue": "1g/data"
			},
			"file": {
				"type": "string",
				"defaultValue": "supplier.tbl"
			}
		},
		"folder": {
			"name": "common"
		},
		"annotations": []
	}
}