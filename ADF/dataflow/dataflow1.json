{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_db_table",
						"type": "DatasetReference"
					},
					"name": "srcdb"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table',",
				"     wildcardPaths:['g1/data/*.tbl']) ~> srcdb",
				"srcdb derive() ~> derivedColumn1"
			]
		}
	}
}